#!/bin/sh
# Copyright Â© 2021 Galois, Inc.  All rights reserved.

exec `dirname $0`/do-configure riscv64-unknown-fromager \
    --buildtype release \
    -Dmultilib=false \
    -Dpicocrt=false \
    -Dio-c99-formats=false \
    -Dio-float-exact=false \
    -Dio-long-double=false \
    -Datomic-ungetc=false \
    -Dnewlib-multithread=false \
    -Dnewlib-nano-malloc=false \
    -Dlibm-long-double=false \
    -Dtests=false \
    -Dspecsdir=none \
    -Dhave-alias-attribute=false \
    --prefix $PWD/image \
    "$@"
